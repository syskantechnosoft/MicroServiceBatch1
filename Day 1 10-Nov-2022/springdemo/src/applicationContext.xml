<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="s1" class="springdemo.Student">
		<property name="id" value="20"></property>
		<property name="name" value="Sample"></property>
		<property name="email" value="sample@gmail.com"></property>
	</bean>
	<bean id="s2" class="springdemo.Student">
		<property name="id" value="30"></property>
		<property name="name" value="test"></property>
		<property name="email" value="test@gmail.com"></property>
	</bean>

	<bean id="s3" class="springdemo.Student">
		<constructor-arg value="50" type="int"></constructor-arg>
		<constructor-arg value="ci"></constructor-arg>
	</bean>
</beans>