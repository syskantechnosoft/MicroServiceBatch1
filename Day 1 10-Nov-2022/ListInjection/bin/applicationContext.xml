<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="q1" class="com.tcs.bean.Question">
		<property name="id" value="1"></property>
		<property name="question" value="What is HTML?"></property>
		<property name="answers">
			<list>
				<value>HTML is HyperText Markup Language</value>
				<value>HTML is a Tag based Language</value>
				<value>HTML is a Language of Web/Internet</value>
				<value>HTML is created by Tim Berners Lee</value>
			</list>
		</property>
	</bean>

</beans>